{
    "name": "peakfijn/get-some-rest",
    "description": "A package to give your Laravel project some REST.",
    "keywords": ["Laravel", "Peakfijn", "Restful", "API", "Rest"],
    "license": "MIT",
    "homepage": "http://github.com/peakfijn/get-some-rest",
    "authors": [
        {
            "name": "Peakfijn BV",
            "email": "hello@peakfijn.nl"
        },
        {
            "name": "Cedric van Putten",
            "email": "me@bycedric.com"
        },
        {
            "name": "Robert van Steen",
            "email": "robert@peakfijn.nl"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "0.6.*",
        "illuminate/contracts": "5.0.*",
        "illuminate/support": "5.0.*",
        "illuminate/http": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "Peakfijn\\GetSomeRest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Peakfijn\\GetSomeRest\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
